{
  "name": "js-library-starter",
  "version": "0.2.0",
  "description": "JavaScript library boilerplate",
  "main": "/lib/js-library-starter.js",
  "scripts": {
    "debug": "node --debug-brk --inspect ./node_modules/.bin/jest -i",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "dev": "webpack --progress --colors --watch --env dev --mode development",
    "build": "webpack --env dev --mode production && webpack --env build --mode production && npm run test ",
    "repl": "local-repl"
  },
  "keywords": [
    "js",
    "library",
    "webpack"
  ],
  "author": "ChangJoo Park",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.3.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-add-module-exports": "^1.0.4",
    "babel-preset-env": "^1.7.0",
    "doctest": "^0.17.1",
    "eslint": "^7.8.1",
    "eslint-config-standard": "^14.1.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^26.4.2",
    "prettier-standard": "^16.4.1",
    "regenerator-runtime": "^0.13.7",
    "standard": "^14.3.4",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "yargs": "^16.0.3",
    "local-repl": "^4.0.0"
  },
  "jest": {
